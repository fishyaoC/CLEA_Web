    <script type="text/javascript">

        var swal_type = ['warning', 'error', 'success', 'question', 'info'];
        var msg_title = '@(TempData["TempMsgTitle"] != null ? TempData["TempMsgTitle"].ToString() : "")';
        var msg_text = '@(TempData["TempMsg"] != null ? TempData["TempMsg"].ToString() : "")';
        var msg_type = '@(TempData["TempMsgType"] != null ? TempData["TempMsgType"].ToString().ToLower() : "")';

        @{
            String msg_text = TempData["TempMsg"] != null ? TempData["TempMsg"].ToString() : "";
        }

                        if ('@Html.Raw(msg_titleF)' != '') {
            Swal.fire({
                confirmButtonColor: '#3085d6',
                confirmButtonText: '確定',
                heightAuto: false,
                icon: '@Html.Raw(msg_typeF)',
                title: '@Html.Raw(msg_titleF)',
                text: '@Html.Raw(msg_textF)'
            });
        }

        if ((typeof msg_text === 'string' && msg_text.trim() != '') || (typeof msg_title === 'string' && msg_title.trim() != '')) {
            Swal.fire({
                confirmButtonColor: '#3085d6',
                confirmButtonText: '確定',
                heightAuto: false,
                icon: msg_type,
                title: msg_title,
                text: '@Html.Raw(msg_text)'
            });
        }
    </script>
     <script type="text/javascript">
        function datetime() {
            //年份選擇器
            $('.year-picker').datepicker({
                startView: 'decade',
                minView: 'decade',
                format: 'yyyy',
                maxViewMode: 2,
                minViewMode: 2,
                autoclose: true
            });
            //日期選擇器
            $('.date-picker').datepicker({
                language: 'zh-TW',
                autoclose: true,
                todayHighlight: true,
            }).next().on(ace.click_event, function () { $(this).prev().focus(); });
            //月份選擇器
            $(".month-picker").datepicker({
                language: 'zh-TW', //(台灣)
                format: "yyyy/mm",
                viewMode: "months",
                minViewMode: "months",
                autoclose: true
            });
            //日期範圍選擇器
            $('.input-daterange').datepicker({ autoclose: true });
        };
         </script>
